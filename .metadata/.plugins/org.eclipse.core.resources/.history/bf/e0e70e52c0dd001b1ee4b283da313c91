package java_prac;

import java.util.Scanner;

public class TravellingSales {
	public static int infinity=100000;
	public TravellingSales (int n,int arr[][]) {
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n,t;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		int arr[][]=new int[n][n];
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i==j){
					
				arr[i][j]=infinity;}
				else{
					
					arr[i][j]=sc.nextInt();
				}
				
			}
		}
		TravellingSales  a=new TravellingSales (n,arr);
		

	}

}
