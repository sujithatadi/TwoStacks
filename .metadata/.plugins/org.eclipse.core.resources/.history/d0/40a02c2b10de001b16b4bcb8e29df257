package wt.controller;

import java.io.IOException;

import javax.servlet.RequestDispatcher;
import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import wt.model.BalancedParanthesisModel;

public class BalancedParanthesis extends HttpServlet{
	public void doPost(HttpServletRequest request, HttpServletResponse response) throws IOException, ServletException{
		String input=request.getParameter("input");
		BalancedParanthesisModel bal=new BalancedParanthesisModel(input);
		request.setAttribute("input", bal.out1);
		request.setAttribute("output", bal.out2);
		RequestDispatcher rd = request.getRequestDispatcher("InfixToPrefix.jsp");
		rd.forward(request, response);
	}
}
